<launch>
  <node name="speech_recog" pkg="pocketsphinx" type="asr_test.py" output="screen">
    <remap from="jsgf_audio" to="sphinx_audio" />
    <param name="gram" value="$(find pocketsphinx)/vocab/restaurant"/>			<!-- original voice_cmd-->
    <param name="dict" value="$(find pocketsphinx)/vocab/restaurant.dict"/>		<!-- original voice_cmd.dic-->
    <param name="grammar" value="restaurant"/>						<!-- original voice_cmd-->
    <param name="rule" value="command"/>						<!-- original move2-->
    <param name="lm" value=":default"/>
    <param name="hmm" value=":default"/>
  </node>

  <node name="audio_control" pkg="pocketsphinx" type="send_audio.py" output="screen">
    <param name="input" value=":default"/>
  </node>
</launch>
